<!DOCTYPE html>
<html>
  <head>
    <title>RecyclingApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div>
      <div class="nav-container3">
      
        <p class="nav-item3"><%= link_to 'Home', root_path %></p>
    
      
        <% if user_signed_in? %>
            <p class="nav-item3"><%= link_to "Post new", new_product_path %></p>
            <%#<p class='home-link'><%= link_to 'Dashboard',  </p>%>
            <p class="nav-item3"><%= link_to 'Log out', destroy_user_session_path, method: :delete %></p>
        
        <% else %>
          <p class="nav-item3"><%= link_to 'Log In', new_user_session_path %></p>
          <p class="nav-item3"><%= link_to 'Sign Up', new_user_registration_path %></p>
        <% end %>
    </div>

    <% flash.each do |key, value| %>
      <%= value %>
    <% end %>
      <%= yield %>
  </body>
</html>
